  "require": {
    "doctrine/orm": "^2.4.8",
    "sylius/resource-bundle": "^0.15.0",
    "dominio": "*@dev",
    "symfony/filesystem": "^2.7",
    "symfony/finder": "^2.7",
    "symfony/yaml": "^2.7"
  },
  "require-dev": {
